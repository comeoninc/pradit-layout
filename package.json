{
  "name": "everlast",
  "version": "1.0.0",
  "description": "An Application Shell for generating Angular Applications with Webpack and ES6",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "NODE_ENV=development && webpack-dev-server --devtool=source-map --progress --colors --port=9000 --content-base app",
    "tester": "NODE_ENV=test && karma start",
    "builder": "NODE_ENV=production && node node_modules/.bin/webpack && cp app/index.html dist/index.html"
  },
  "keywords": [
    "angular",
    "webpack",
    "es6"
  ],
  "author": "Sean Larkin",
  "license": "MIT",
